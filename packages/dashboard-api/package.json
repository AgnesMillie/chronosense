{
  "name": "@chronosense/dashboard-api",
  "version": "0.1.0",
  "private": true,
  "description": "The core API for the Chronosense dashboard and data ingestion.",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "start:dev": "nodemon src/server.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "express": "^4.17.1",
    "pg": "^8.16.3"
  },
  "author": "AgnesMillie",
  "devDependencies": {
    "@types/pg": "^8.15.5",
    "dotenv": "^17.2.3"
  }
}
